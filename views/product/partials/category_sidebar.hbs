<div class="sidebar-widget category">
    <h2 class="title">Category</h2>
    <nav class="navbar bg-light">
        <ul class="navbar-nav">
            {{#each category}}
                <li class="nav-item">
                    <a class="nav-link" href="/products?catalog={{id}}">
                    <i class="fa fa-angle-double-right"></i>{{name}}
                    </a>
                </li>
            {{/each}}
        </ul>
    </nav>
</div>