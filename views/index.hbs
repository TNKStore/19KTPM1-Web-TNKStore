<!-- Main Slider Start -->
<div class="header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <nav class="navbar bg-light">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="home"><i class="fa fa-home"></i>Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#"><i class="fa fa-shopping-bag"></i>Best Selling</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#"><i class="fa fa-plus-square"></i>New Arrivals</a>
                        </li>
                        {{#list category class="nav-item"}}
                            <a class="nav-link" href="product-list"><i class="fa fa-female"></i>{{name}}</a>
                        {{/list}}
                    </ul>
                </nav>
            </div>
            <div class="col-md-6">
                <div class="header-slider normal-slider">
                    <div class="header-slider-item" href="product-list">
                        <img src="https://genk.mediacdn.vn/2017/8-1498811649212.jpg" alt="Slider Image"/>
                        <div class="header-slider-caption">
                            <p>Some text goes here that describes the image</p>
                            <a class="btn" href=""><i class="fa fa-shopping-cart"></i>Shop Now</a>
                        </div>
                    </div>
                    <div class="header-slider-item" href="product-list">
                        <img src="https://chothumua.vn/image/files/600x600/laptop-gaming-gia-re-phong-vu-6.jpg" alt="Slider Image"/>
                        <div class="header-slider-caption">
                            <p>Some text goes here that describes the image</p>
                            <a class="btn" href=""><i class="fa fa-shopping-cart"></i>Shop Now</a>
                        </div>
                    </div>
                    <div class="header-slider-item" href="product-list">
                        <img src="https://laptopcuongphat.com/hoanghung/5/images/seo/3/laptop-hang-nao-ben-3.jpg" alt="Slider Image"/>
                        <div class="header-slider-caption">
                            <p>Some text goes here that describes the image</p>
                            <a class="btn" href=""><i class="fa fa-shopping-cart"></i>Shop Now</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="header-img">
                    <div class="img-item">
                        {{#with category.[0]}}
                            <img src="{{image}}"/>
                            <a class="img-text" href="">
                                <p>{{name}}</p>
                            </a>
                        {{/with}}
                    </div>
                    <div class="img-item">
                        {{#with category.[1]}}
                            <img src="{{image}}"/>
                            <a class="img-text" href="">
                                <p>{{name}}</p>
                            </a>
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Main Slider End -->

