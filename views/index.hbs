{{> header homeActive=" active"}}

<!-- Main Slider Start -->
<div class="header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <nav class="navbar bg-light">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="/home"><i class="fa fa-home"></i>Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#"><i class="fa fa-shopping-bag"></i>Best Selling</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#"><i class="fa fa-plus-square"></i>New Arrivals</a>
                        </li>
                        {{#each category}}
                            <li class="nav-item">
                                <a class="nav-link" href="/products?catalog={{id}}">
                                <i class="fa fa-angle-double-right"></i>{{name}}
                                </a>
                            </li>
                        {{/each}}
                    </ul>
                </nav>
            </div>
            <div class="col-md-6">
                <div class="header-slider normal-slider">
                    <div class="header-slider-item" href="/products">
                        <img src="https://genk.mediacdn.vn/2017/8-1498811649212.jpg" alt="Slider Image"/>
                        <div class="header-slider-caption">
                            <p>Chào mừng bạn đến với TNK Store!</p>
                            <a class="btn" href="/products"><i class="fa fa-shopping-cart"></i>Shop Now</a>
                        </div>
                    </div>
                    <div class="header-slider-item" href="/products">
                        <img src="https://chothumua.vn/image/files/600x600/laptop-gaming-gia-re-phong-vu-6.jpg"
                             alt="Slider Image"/>
                        <div class="header-slider-caption">
                            <p>Click "Shop now" để bắt đầu mua sắm</p>
                            <a class="btn" href="/products"><i class="fa fa-shopping-cart"></i>Shop Now</a>
                        </div>
                    </div>
                    <div class="header-slider-item" href="/products">
                        <img src="https://admin.oll.co/uploads/course/course_Untitled%20design%20(4)%20(1)_10130310112020000000.png"
                             alt="Slider Image"/>
                        <div class="header-slider-caption">
                            <p>Click "Shop now" để bắt đầu mua sắm</p>
                            <a class="btn" href="/products"><i class="fa fa-shopping-cart"></i>Shop Now</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="header-img">
                    <div class="img-item">
                        <img src="https://images.prismic.io/frameworkmarketplace/cca31de3-3b75-4932-af96-7646b7eba6c7__DSC3630-Edit-cropped.jpg?auto=compress,format"/>
                        <a class="img-text" href="/products">
                            <p>{{name}}</p>
                        </a>
                    </div>
                    <div class="img-item">
                        <img src="https://images.prismic.io/frameworkmarketplace/5d87bd50-8e78-4a7a-ade9-4356e3693416_Pre-Marketplace+-+image_02.jpg?auto=compress,format"/>
                        <a class="img-text" href="/products">
                            <p>{{name}}</p>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Main Slider End -->

<!-- Brand Start -->
<!--{{> brand_slide brand=brand}}-->
<!-- Brand End -->

<!-- Feature Start-->
<div class="feature">
    <div class="container-fluid">
        <div class="row align-items-center">
            <div class="col-lg-3 col-md-6 feature-col">
                <div class="feature-content">
                    <i class="fab fa-cc-mastercard"></i>
                    <h2>Secure Payment</h2>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 feature-col">
                <div class="feature-content">
                    <i class="fa fa-truck"></i>
                    <h2>Worldwide Delivery</h2>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 feature-col">
                <div class="feature-content">
                    <i class="fa fa-sync-alt"></i>
                    <h2>90 Days Return</h2>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 feature-col">
                <div class="feature-content">
                    <i class="fa fa-comments"></i>
                    <h2>24/7 Support</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Feature End-->

<!-- Category Start-->
<div class="category">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3">
                <div class="category-item ch-400">
                    {{#with category.[0]}}
                        <img src="https://cdn.tgdd.vn/Products/Images/44/247526/asus-zenbook-ux325ea-i5-kg363t-16-600x600.jpg"/>
                        <a class="category-name" href="/products?catalog={{id}}">
                        <p>{{name}}</p>
                        </a>
                    {{/with}}
                </div>
            </div>
            <div class="col-md-3">
                <div class="category-item ch-250">
                    {{#with category.[1]}}
                        <img src="https://cellphones.com.vn/sforum/wp-content/uploads/2019/09/acer-aspire-5-129.jpg"/>
                        <a class="category-name" href="/products?catalog={{id}}">
                        <p>{{name}}</p>
                        </a>
                    {{/with}}
                </div>
                <div class="category-item ch-150">
                    {{#with category.[2]}}
                        <img src="https://img.websosanh.vn/v10/users/review/images/vsmzqgcy7huqq/1568025192558_2268433.jpg?compress=85"/>
                        <a class="category-name" href="/products?catalog={{id}}">
                        <p>{{name}}</p>
                        </a>
                    {{/with}}
                </div>
            </div>
            <div class="col-md-3">
                <div class="category-item ch-150">
                    {{#with category.[3]}}
                        <img src="https://cellphones.com.vn/sforum/wp-content/uploads/2020/11/ENnqVCdUYAQ-LOA.jpg"/>
                        <a class="category-name" href="/products?catalog={{id}}">
                        <p>{{name}}</p>
                        </a>
                    {{/with}}
                </div>
                <div class="category-item ch-250">
                    {{#with category.[4]}}
                        <img src="https://salt.tikicdn.com/ts/tmp/61/1f/39/623d5d1320108183a61a74a68b01628b.jpg"/>
                        <a class="category-name" href="/products?catalog={{id}}">
                        <p>{{name}}</p>
                        </a>
                    {{/with}}
                </div>
            </div>
            <div class="col-md-3">
                <div class="category-item ch-400">
                    {{#with category.[5]}}
                        <img src="https://st.quantrimang.com/photos/image/2021/02/06/top-ban-phim-co-1.jpg"/>
                        <a class="category-name" href="/products?catalog={{id}}">
                        <p>{{name}}</p>
                        </a>
                    {{/with}}
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Category End-->

<!-- Call to Action Start -->
<div class="call-to-action">
    <div class="container-fluid">
        <div class="row align-items-center">
            <div class="col-md-6">
                <h1>CALL US FOR ANY QUERIES</h1>
            </div>
            <div class="col-md-6">
                <span>0909080808</span>
            </div>
        </div>
    </div>
</div>
<!-- Call to Action End -->

<!-- Featured Product Start -->
<div class="featured-product product">
    <div class="container-fluid">
        <div class="section-header">
            <h1>Featured Product</h1>
        </div>
        <div class="row align-items-center product-slider product-slider-4">
            {{#each products}}
                <div class="col-lg-3">
                    {{> product product=this}}
                </div>
            {{/each}}
        </div>
    </div>
</div>
<!-- Featured Product End -->

<!-- Recent Product Start -->
<div class="recent-product product">
    <div class="container-fluid">
        <div class="section-header">
            <h1>Recent Product</h1>
        </div>
        <div class="row align-items-center product-slider product-slider-4">
            {{#each products}}
                <div class="col-lg-3">
                    {{> product product=this}}
                </div>
            {{/each}}
        </div>
    </div>
</div>
<!-- Recent Product End -->


{{> footer}}
